 public void MaxiProduct(){
        int [] nums={-2};

        int intialmax=nums[0];
        int max=nums[0];
        for (int i=1;i<nums.length;i++){

            if(nums[i]>intialmax*nums[i]){
                intialmax=nums[i];
                            }
            else{
                intialmax*=nums[i];
            }


            if(max<intialmax){
                max=intialmax;
            }


        }
        System.out.println(max);
    }
