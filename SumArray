   public  void SumArray(){
        int [] num={15, 2, 4, 8, 9, 5, 10, 23};
        int k=23;
        for(int i=0;i<num.length-1;i++){
            int sum=0;
            for(int j=i;j<num.length;j++){
                sum+=num[j];
                if(sum==k){
                    System.out.println(num[i]+" "+num[j]);
                }
            }
        }
    }
