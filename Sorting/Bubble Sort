   static void bubblesort(){
        int [] nums={2,4,7,13,3,6,96,36,8,75,43};
        
        for(int i=0;i<nums.length-1;i++){
            for(int j=0;j<nums.length-i-1;j++){
                if(nums[j+1]<nums[j]){
                    int temp=nums[j];
                    nums[j]=nums[j+1];
                    nums[j+1]=temp;
                }
            }
        }
        for(int x:nums){
            System.out.println(x);
        }
    }
    
