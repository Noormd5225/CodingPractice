    static void combinationusingloop(){
        int [] nums={1,5,6,3,57,30,70,46,54,6,8,6,7,9,4,43,56,87,0};
        for(int i=0;i<nums.length-1;i++){
            
            for(int j=i+1;j<nums.length;j++){
                if(nums[i]+nums[j]==100){
                    System.out.println(nums[i]+" "+nums[j]);
                }
            }
        }
    }
    
