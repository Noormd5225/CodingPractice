 public static void ToLowerCase(){
        String str = "FiNe";
        String goal="";
        for(int i=0;i<str.length();i++){
            if(str.charAt(i)>=65&&str.charAt(i)<=90 ){
                goal+= (char) (str.charAt(i)+32);
            }
            else{
                goal+= str.charAt(i);
            }
        }
        System.out.println(goal);
    }
